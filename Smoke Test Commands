Kubectl cluster-info
kubectl get svc
kubectl get nodes
kubectl get pods
kubectl get pods -A
kubectl get pods -o wide
kubectl describe node name
kubectl run pod testpod1 --image=nginx:latest
kubectl create deployment testdeploy1 --image=nginx:latest --replicas 3
kubectl expose testdeploy1 --port=8000 --target-port=80 --type-NodePort
